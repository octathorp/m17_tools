		clocks = <0x3 0xc5 0x3 0x1ce>;
		clock-names = "aclk_vcodec", "hclk_vcodec";
		resets = <0x3 0x71 0x3 0x70>;
		reset-names = "video_h", "video_a";
		power-domains = <0x19 0x2>;
		dev_mode = <0x0>;
		iommus = <0x22>;
		allocator = <0x1>;
		status = "okay";
		phandle = <0x23>;
	};

	iommu@10106800 {
		compatible = "rockchip,iommu";
		reg = <0x10106800 0x40>;
