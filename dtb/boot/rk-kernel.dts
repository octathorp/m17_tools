		gpio = <0x3b 0xa 0x0>;
		regulator-name = "vcc50_boost";
		enable-active-high;
		regulator-min-microvolt = <0x4c4b40>;
		regulator-max-microvolt = <0x4c4b40>;
		regulator-always-on;
		regulator-boot-on;
		status = "okay";
		phandle = <0xac>;
	};

	vdd-arm-regulator {
		compatible = "pwm-regulator";
		rockchip,pwm_id = <0x0>;
		rockchip,pwm_voltage = <0x10c8e0>;
		pwms = <0x5e 0x0 0x1388 0x1>;
		regulator-name = "vdd_arm";
		regulator-min-microvolt = <0xe7ef0>;
		regulator-max-microvolt = <0x16e360>;
		regulator-always-on;
		regulator-boot-on;
		phandle = <0x4>;
	};

	vdd-log-regulator {
		compatible = "pwm-regulator";
		rockchip,pwm_id = <0x1>;
		rockchip,pwm_voltage = <0x10c8e0>;
		pwms = <0x5f 0x0 0x1388 0x1>;
		regulator-name = "vdd_log";
		regulator-min-microvolt = <0xde2b0>;
		regulator-max-microvolt = <0x13d620>;
		regulator-always-on;
		regulator-boot-on;
		phandle = <0x15>;
	};

